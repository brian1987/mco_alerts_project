<h1>Listing alerts</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Dob</th>
      <th>Medicaid</th>
      <th>Alert</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alerts.each do |alert| %>
      <tr>
        <td><%= alert.first_name %></td>
        <td><%= alert.last_name %></td>
        <td><%= alert.dob %></td>
        <td><%= alert.medicaid_id %></td>
        <td><%= alert.alert %></td>
        <td><%= link_to 'Show', alert %></td>
        <td><%= link_to 'Edit', edit_alert_path(alert) %></td>
        <td><%= link_to 'Destroy', alert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alert', new_alert_path %>


<%= form_tag import_alerts_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Submit" %>
<% end %>
